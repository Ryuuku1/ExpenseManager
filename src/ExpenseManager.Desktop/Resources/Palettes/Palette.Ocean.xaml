<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="BackgroundColor">#FF0D1824</Color>
    <Color x:Key="SidebarColor">#FF09111B</Color>
    <Color x:Key="CardColor">#FF142335</Color>
    <Color x:Key="CardBorderColor">#FF1C2F46</Color>
    <Color x:Key="SurfaceAltColor">#FF193049</Color>
    <Color x:Key="SurfaceAltBorderColor">#FF24415D</Color>
    <Color x:Key="InputBackgroundColor">#FF10263A</Color>
    <Color x:Key="InputBorderColor">#FF1D3A52</Color>
    <Color x:Key="ListItemBackgroundColor">#FF193049</Color>
    <Color x:Key="ListItemHoverColor">#33008FD6</Color>
    <Color x:Key="ListItemSelectedColor">#55008FD6</Color>
    <Color x:Key="TableRowBackgroundColor">#FF1A2F46</Color>
    <Color x:Key="TableRowAlternateColor">#FF203854</Color>
    <Color x:Key="TableRowHoverColor">#33008FD6</Color>
    <Color x:Key="TableRowSelectedColor">#55008FD6</Color>
    <Color x:Key="AccentColor">#FF008FD6</Color>
    <Color x:Key="AccentHoverColor">#FF1CA6E4</Color>
    <Color x:Key="PrimaryTextColor">#FFFFFFFF</Color>
    <Color x:Key="SecondaryTextColor">#FFD2ECFF</Color>
    <Color x:Key="MutedTextColor">#FFA3D4F0</Color>
    <Color x:Key="ChartBackgroundColor">#FF1E3148</Color>
    <Color x:Key="ChartPlotAreaColor">#FF16263B</Color>
    <Color x:Key="ChartGridlineColor">#33008FD6</Color>
    <Color x:Key="ChartCardColor">#FF1E3148</Color>
    <Color x:Key="ChartCardBorderColor">#FF2A4760</Color>
    <Color x:Key="ScrollbarThumbColor">#FF244B67</Color>
    <Color x:Key="ShadowColor">#66000000</Color>

    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource BackgroundColor}" />
    <SolidColorBrush x:Key="SidebarBrush" Color="{StaticResource SidebarColor}" />
    <SolidColorBrush x:Key="CardBrush" Color="{StaticResource CardColor}" />
    <SolidColorBrush x:Key="CardBorderBrush" Color="{StaticResource CardBorderColor}" />
    <SolidColorBrush x:Key="SurfaceAltBrush" Color="{StaticResource SurfaceAltColor}" />
    <SolidColorBrush x:Key="SurfaceAltBorderBrush" Color="{StaticResource SurfaceAltBorderColor}" />
    <SolidColorBrush x:Key="InputBackgroundBrush" Color="{StaticResource InputBackgroundColor}" />
    <SolidColorBrush x:Key="InputBorderBrush" Color="{StaticResource InputBorderColor}" />
    <SolidColorBrush x:Key="ListItemBackgroundBrush" Color="{StaticResource ListItemBackgroundColor}" />
    <SolidColorBrush x:Key="ListItemHoverBrush" Color="{StaticResource ListItemHoverColor}" />
    <SolidColorBrush x:Key="ListItemSelectedBrush" Color="{StaticResource ListItemSelectedColor}" />
    <SolidColorBrush x:Key="TableRowBackgroundBrush" Color="{StaticResource TableRowBackgroundColor}" />
    <SolidColorBrush x:Key="TableRowAlternateBrush" Color="{StaticResource TableRowAlternateColor}" />
    <SolidColorBrush x:Key="TableRowHoverBrush" Color="{StaticResource TableRowHoverColor}" />
    <SolidColorBrush x:Key="TableRowSelectedBrush" Color="{StaticResource TableRowSelectedColor}" />
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}" />
    <SolidColorBrush x:Key="AccentHoverBrush" Color="{StaticResource AccentHoverColor}" />
    <SolidColorBrush x:Key="PrimaryTextBrush" Color="{StaticResource PrimaryTextColor}" />
    <SolidColorBrush x:Key="SecondaryTextBrush" Color="{StaticResource SecondaryTextColor}" />
    <SolidColorBrush x:Key="MutedTextBrush" Color="{StaticResource MutedTextColor}" />
    <SolidColorBrush x:Key="ChartBackgroundBrush" Color="{StaticResource ChartBackgroundColor}" />
    <SolidColorBrush x:Key="ChartPlotAreaBrush" Color="{StaticResource ChartPlotAreaColor}" />
    <SolidColorBrush x:Key="ChartGridlineBrush" Color="{StaticResource ChartGridlineColor}" />
    <SolidColorBrush x:Key="ChartCardBrush" Color="{StaticResource ChartCardColor}" />
    <SolidColorBrush x:Key="ChartCardBorderBrush" Color="{StaticResource ChartCardBorderColor}" />
    <SolidColorBrush x:Key="ScrollbarThumbBrush" Color="{StaticResource ScrollbarThumbColor}" />
    <SolidColorBrush x:Key="ShadowBrush" Color="{StaticResource ShadowColor}" />
</ResourceDictionary>
